<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 运行/存储/通知等权限（生成器注入） -->



    <application
        android:name=".App"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Theme.NDJC"
        android:localeConfig="@xml/locale_config"
        >

        <!-- 应用图标/启动图标占位 -->



        <!-- 网络安全 xml（若启用可指向 @xml/network_security_config） -->



        <!-- 前台服务类型（如 location/media 等） -->



        <!-- FileProvider / Shortcuts / Widget 占位 -->




        <!-- 频道/环境 meta-data（依靠 manifestPlaceholders） -->
        <meta-data android:name="ndjc.meta.CHANNEL" android:value="${META_CHANNEL}" />
        <meta-data android:name="ndjc.meta.ENV"      android:value="${META_ENV}" />

        <!-- Maps 自定义样式（如使用，可在此注入） -->


        <activity
            android:name=".MainActivity"
            android:exported="true">


            <!-- 入口 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!-- 额外 IntentFilter / Deeplinks / App Links -->



            <!-- 深链 -->



        </activity>

        <!-- 开机/通知等接收器（生成器注入处） -->



    </application>
</manifest>
