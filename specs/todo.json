{
  "template": "simple-template",
  "feature": "todo",
  "title": "My Todos",
  "button": "Fetch",
  "package": "com.ndjc.app",
  "permissions": ["android.permission.INTERNET"],
  "client": "retrofit",
  "baseUrl": "https://jsonplaceholder.typicode.com",
  "endpoints": [
    {
      "name": "getTodo",
      "method": "GET",
      "path": "/todos/{id}",
      "pathParams": ["id"],
      "responseModel": {
        "name": "Todo",
        "fields": {
          "userId": "Int",
          "id": "Int",
          "title": "String",
          "completed": "Boolean"
        }
      }
    }
  ]
}
